<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.Nimal_Inventario_Simples_Yapp" version="1.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Keyboard">
        <param name="android-package" value="io.ionic.keyboard.IonicKeyboard" />
        <param name="onload" value="true" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <name>Nimal Inventario Simples</name>
    <description>
      Aplicativo desenvolvido por Yasmin Feitosa
    </description>
    <author email="feitosa.y@gmail.com" href="starter.pe.hu">
      Yasmin de Lima Feitosa
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <icon density="ldpi" src="resources/android/icon/logo-nimal-160.png" />
    <icon density="mdpi" src="resources/android/icon/logo-nimal-160.png" />
    <icon density="hdpi" src="resources/android/icon/logo-nimal-160.png" />
    <icon density="xhdpi" src="resources/android/icon/logo-nimal-160.png" />
    <icon density="xxhdpi" src="resources/android/icon/logo-nimal-160.png" />
    <icon density="xxxhdpi" src="resources/android/icon/logo-nimal-160.png" />
    <splash density="land-ldpi" src="resources/android/splash/logo-nimal.png" />
    <splash density="land-mdpi" src="resources/android/splash/logo-nimal.png" />
    <splash density="land-hdpi" src="resources/android/splash/logo-nimal.png" />
    <splash density="land-xhdpi" src="resources/android/splash/logo-nimal.png" />
    <splash density="land-xxhdpi" src="resources/android/splash/logo-nimal.png" />
    <splash density="land-xxxhdpi" src="resources/android/splash/logo-nimal.png" />
    <splash density="port-ldpi" src="resources/android/splash/logo-nimal.png" />
    <splash density="port-mdpi" src="resources/android/splash/logo-nimal.png" />
    <splash density="port-hdpi" src="resources/android/splash/logo-nimal.png" />
    <splash density="port-xhdpi" src="resources/android/splash/logo-nimal.png" />
    <splash density="port-xxhdpi" src="resources/android/splash/logo-nimal.png" />
    <splash density="port-xxxhdpi" src="resources/android/splash/logo-nimal.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
</widget>
